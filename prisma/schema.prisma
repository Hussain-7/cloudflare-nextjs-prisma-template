generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Brainlift {
  id      String @id @default(cuid())
  purpose String
  dok4    String
  dok3    String
  experts String
  owners  String
  
  @@map("brainlifts")
}
