{
	"name": "brainlift-studio",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"migrate": "dotenv -e .dev.vars -- pnpm prisma migrate dev",
		"generate": "dotenv -e .dev.vars -- pnpm prisma generate --no-engine",
		"studio": "dotenv -e .dev.vars -- pnpm prisma studio",
		"seed": "dotenv -e .dev.vars -- pnpm prisma db seed",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.3.0",
		"@prisma/client": "6.16.2",
		"@prisma/extension-accelerate": "^2.0.2",
		"@tanstack/react-query": "^5.90.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.544.0",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-hook-form": "^7.63.0",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.1.11"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.17",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"dotenv-cli": "^10.0.0",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"prisma": "^6.16.2",
		"tailwindcss": "^4",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5",
		"wrangler": "^4.40.2"
	}
}
